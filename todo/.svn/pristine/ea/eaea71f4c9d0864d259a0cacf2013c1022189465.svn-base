body{
    font-size:14px;
}
.page-body{
    margin-top:50px;
}
.clear-fix{ clear:both}
.page-body .egg-close{
    background: url(../img/list/egg-close.jpg) no-repeat;
    background-size: 100%;
    position: absolute;
    top: 20%;
    left: 2%;
    width: 14%;
    height: 11%;
    display: none;
}
.page-body .list-tip-style{
    padding:20px;
}


.page-body .list-input .list-inp{
    float:left;
    width:80%;
}


.btn-input-close:focus, .btn-input-close:hover  {
    color: #000;
    text-decoration: none;
    cursor: pointer;
    filter: alpha(opacity=50);
    opacity: .5;
}
/*小屏手机*/
@media screen and (max-width: 253px){
    .imgAnimation{
        animation-name: imgAnimation;
        animation: imgAnimation 2s ease;
        -webkit-animation: imgAnimation 2s ease;
        -moz-animation: imgAnimation 2s ease;
    }

    @-webkit-keyframes imgAnimation {
        0%{ left: -198px}
        100%{ left: 140px;}
    }

    @-moz-keyframes imgAnimation{
        0%{ left: -198px}
        100%{ left: 140px;}
    }
    .click-picture-tip .tip-d {
        position: relative;
        font-size: 20px;
        font-family: "Microsoft YaHei", 微软雅黑, "MicrosoftJhengHei", 华文细黑, STHeiti, MingLiu, Arial, Helvetica, sans-serif;
        color: #000;
        background-color: #ffd700;
        border-radius:7px;
        padding: 10px;
    }
    .left_triangle{
        height:0px;
        width:0px;
        border-width:10px;
        border-style:solid;
        border-color:transparent  #FFD700 transparent transparent ;
        position: absolute;
        left: -19px;
        top:3px;
    }
    .page-body .click-picture{
        background : url(../img/list/hen.png) no-repeat;
        background-size: contain;
        width: 34%;
        height: 130px;
        margin-left: 10px;

    }
    .page-body .list-input .list-pic{
        padding-top: 50px;
        width: 14%;
        background: url(../img/list/egg-close.jpg) no-repeat;
        background-size: contain;
        float: left;
        margin: 2%;
    }
    .page-body .list-input .list-title{
        padding: 6px 12px;
        margin-left: 4px;
        padding-top: 10px;
        padding-bottom: 10px;
        width: 94%;
        background: #ffff67;
        border:6px #fdd301 solid;
        margin-bottom: -18px;
    }
    .btn-input-close {
        opacity: .4;
        float: right;
        font-size: 18px;
        font-weight: 400;
        color: #000;
        text-shadow: 0 1px 0 #fff;
        filter: alpha(opacity=20);
        -webkit-appearance: none;
        padding: 0;
        cursor: pointer;
        background: 0 0;
        border: 0;
        position: relative;
        top: -16px;
        right: 26px;
    }
    .page-body .list-input {
        margin-top: 0px;
    }
    .egg-red, .egg-orange, .egg-blue, .egg-green{
        font-size: 34px;
        float: left;
        margin-left: 0;
    }
    .all-page{
        margin-left: 4px;
        margin-bottom: 80px;
    }
    .list-main{
        margin-top: 20px;
    }
    .search-no-finish,.search-finish,.search-refresh{
        font-size: 22px;
        float: left;
        margin-left: 1%;
        margin-top: 7px;
        color: #fff;
        font-weight: 900;
    }
    .click-picture-tip {
        position: absolute;
        left: 140px;
        top: 9%;
        font-size: 24px;
        transform: rotate(-20deg);

    }
    .list-search-time{
        float: left;
        font-size: 12px;
        margin-top: 10px;
        padding: 3px;
        border: 0;
        background: #4CAB5A;
        width: 20%;
        color: #fff;
    }
    .start-time{
        margin-left: 6px;
    }
    .end-time{
        margin-left: 3px;
    }
    .do-start-time{
        margin-left: 5%;
    }
    .do-end-time{
        margin-left: 3px;
    }
    .btn-go{
        float: left;
        width: 8%;
        margin-top: 10px;
        margin-left: 4px;
        padding: 2px 0;
        background: #4CAB5A;
    }
    .page-body .list-input .list-inp {
        float: left;
        width: 76%;
    }
}

