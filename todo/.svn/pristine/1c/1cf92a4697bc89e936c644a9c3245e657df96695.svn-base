@charset "UTF-8";
.notes-add img {
    position: fixed;
    width: 80px;
    bottom: 14px;
    left: 20px;
}
.notes {
    margin-top: 50px;
    margin-left: 2px;
    color: #000;
    padding-bottom: 130px;
}
/*iphone 5*/
@media screen and (max-width: 320px){
    .paper1 {
        /*min-height: 150px;*/
        width: 50%;
    }
    .paper2 {
        width: 50%;
    }
    .paper3 {
        min-height: 150px;
    }
    .paper4{
        min-height: 150px;
    }
    .paper1 p {
        padding: 50px 10px 10px 10px;
    }
    .paper2 p {
        padding: 46px 20px 10px 23px;
    }
    .paper3 p{
        padding: 76px 10px 10px 10px;

    }
    .paper4 p {
        padding: 28px 15px;
    }
}
/*iphone 6*/
@media screen and (min-width: 321px) and (max-width: 1600px) {
    .paper1 {
       min-height: 230px;
       min-width: 200px;
    }
    .paper2 {
        min-height: 230px;
        min-width: 200px;
        margin-left: -20px;
        margin-top: 16px;
    }
    .paper3 {
        min-height: 230px;
        min-width: 200px;
    }
    .paper4{
        min-height: 210px;
        min-width: 200px;
        margin-top: 20px;
    }
    .paper1 p {
        padding: 50px 10px 10px 10px;
    }
    .paper2 p {
        padding: 46px 20px 10px 23px;
    }
    .paper3 p{
        padding: 76px 10px 10px 10px;

    }
    .paper4 p {
        padding: 28px 15px;
    }

}
